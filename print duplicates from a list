


def repeat(x):
    _size = len(x)
    repeated = []
    for i in range (_size):
        k = i+1
        for j in range (k, _size):
            if x[i] == x[j] and x[i] not in repeated :
                repeated.append(x[i])
    return repeated

list = [10,20,30,40,20,30,50,60,70,50,80,90,40]
print(repeat(list))        